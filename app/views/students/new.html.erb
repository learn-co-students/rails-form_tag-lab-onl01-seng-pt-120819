<label> Student Form </label><br>

<%=  form_tag students_path do %>
    <label> First Name </label><br>
    <%= text_field_tag :'student[first_name]' %> <br>
    <label> Last Name </label><br>
    <%= text_field_tag :'student[last_name]' %>
    <%= submit_tag "Submit Student" %>
<% end %>
<%= params.inspect %>